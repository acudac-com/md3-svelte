{
	"name": "@acudac/md3-svelte",
	"version": "1.1.2",
	"type": "module",
	"repository": "acudac-com/md3-svelte",
	"author": {
		"name": "DanielvNiek"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package -o package",
		"package-components": "vite build -c vite.config.lib.js",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"theme": "find src/theme -type f -exec sed -i '' -e 's/rgb(//g' -e 's/);/;/g' {} +"
	},
	"exports": {
		".": {
			"types": "./package/index.d.ts",
			"svelte": "./package/index.ts"
		},
		"./misc/styles.css": {
			"import": "./package/misc/styles.css"
		}
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/package": "^2.3.10",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/typography": "^0.5.16",
		"autoprefixer": "^10.4.20",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.10",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.0.0",
		"vite": "^6.0.0"
	},
	"packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af",
	"dependencies": {
		"bits-ui": "^1.3.5",
		"mode-watcher": "^0.5.1",
		"tailwind-merge": "^3.0.2",
		"@sveltejs/kit": "^2.16.0",
		"svelte": "^5.0.0"
	}
}
